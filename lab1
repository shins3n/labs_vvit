number=int(input('Введите число:'))

# for i in range(1,number+1):
#     print(i)
i=1
while i <= number:
    print(i)
    i+=1
#2
number=int(input('Введите первое число:'))
number2=int(input('Введите второе число:'))
if number>number2:
    print(number)
elif number2>number:
    print(number)
elif number == number2:
    print('Числа равны=',number)
